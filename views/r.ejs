<!doctype html><html lang="en"><head><meta charset="utf-8" /><meta http-equiv="refresh" content="0.5; url=<%=destination%>"><title></title>

<script>
function redirectNow(){ window.location.href = '<%= destination %>'; };
function timeoutRedirect(){ setTimeout(redirectNow, 5000); };
timeoutRedirect();
</script>
</head>
<body onload="timeoutRedirect();">
<noscript>Redirecting to <a href="<%=destination%>"><%=destination%></a></noscript>

<% if (facebook) { %>
<!-- Facebook Pixel -->
<script>!function(f, b, e, v, n, t, s) {if (f.fbq) return; n = f.fbq = function() {n.callMethod ? n.callMethod.apply(n, arguments) : n.queue.push(arguments) }; if (!f._fbq) f._fbq = n; n.push = n; n.loaded = !0; n.version = '2.0'; n.queue = []; t = b.createElement(e); t.async = !0; t.src = v; s = b.getElementsByTagName(e)[0]; s.parentNode.insertBefore(t, s) }(window, document, 'script', 'https://connect.facebook.net/en_US/fbevents.js'); fbq('init', '<%=facebook%>'); fbq('track', 'PageView');</script>
<noscript><img height="1" width="1" style="display:none" src="https://www.facebook.com/tr?id=<%= facebook %>&ev=PageView&noscript=1" /></noscript>
<% } %>

<% if (twitter) { %>
<!-- Twitter Pixel -->
<script>!function(e, t, n, s, u, a) {e.twq || (s = e.twq = function() {s.exe ? s.exe.apply(s, arguments) : s.queue.push(arguments); }, s.version = '1.1', s.queue = [], u = t.createElement(n), u.async = !0, u.src = '//static.ads-twitter.com/uwt.js', a = t.getElementsByTagName(n)[0], a.parentNode.insertBefore(u, a)) }(window, document, 'script'); twq('init', '<%=twitter%>'); twq('track', 'PageView');</script>
<% } %>

<% if (linkedin) { %>
<!-- Linkedin -->
<script type="text/javascript">_linkedin_data_partner_id = "<%=linkedin%>";</script>
<script type="text/javascript">(function() {var s = document.getElementsByTagName("script")[0]; var b = document.createElement("script"); b.type = "text/javascript"; b.async = true; b.src = "https://snap.licdn.com/li.lms-analytics/insight.min.js"; s.parentNode.insertBefore(b, s); })(); </script>
<noscript><img height="1" width="1" style="display:none;" alt="" src="https://dc.ads.linkedin.com/collect/?pid=<%=linkedin%>fmt=gif" /></noscript>
<% } %>

<% if (google) { %>
<!-- Google Pixel -->
<script type="text/javascript">var google_conversion_id = "<%=google%>";var google_custom_params = window.google_tag_params; var google_remarketing_only = true; </script>
<script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js"></script>
<noscript><div style="display:inline;"><img height="1" width="1" style="border-style:none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/<%=google%>/?guid=ON&amp;script=0" /></div></noscript>
<% } %>

</body>
</html>
